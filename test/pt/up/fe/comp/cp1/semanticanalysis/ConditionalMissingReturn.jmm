class ConditionalMissingReturn {
    public int absoluteValue(int x) {
        if (x < 0) {
            return x;
        }
        // no return when x >= 0
    }

    public static void main(String[] args) {
        ConditionalMissingReturn obj;
        int abs;

        obj = new ConditionalMissingReturn();
        abs = obj.absoluteValue(-5);
    }
}
